# ------------START-CODER-----------
# This section is managed by coder. DO NOT EDIT.
#
# You should not hand-edit this section unless you are removing it, all
# changes will be lost when running "coder config-ssh".
#
Host coder.*
	ForwardAgent yes
	ServerAliveCountMax 3
	ServerAliveInterval 60
	ConnectTimeout=0
	StrictHostKeyChecking=no
	UserKnownHostsFile=/dev/null
	LogLevel ERROR
	ProxyCommand /Users/andyhamon/discord/discord/.local/bin/coder --global-config "/Users/andyhamon/Library/Application Support/coderv2" ssh --stdio --ssh-host-prefix coder. %h

Host *.coder
	ForwardAgent yes
	ServerAliveCountMax 3
	ServerAliveInterval 60
	ConnectTimeout=0
	StrictHostKeyChecking=no
	UserKnownHostsFile=/dev/null
	LogLevel ERROR

Match host *.coder !exec "/Users/andyhamon/discord/discord/.local/bin/coder connect exists %h"
	ProxyCommand /Users/andyhamon/discord/discord/.local/bin/coder --global-config "/Users/andyhamon/Library/Application Support/coderv2" ssh --stdio --hostname-suffix coder %h
# ------------END-CODER------------
